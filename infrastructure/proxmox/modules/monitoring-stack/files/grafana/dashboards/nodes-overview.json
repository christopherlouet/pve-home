                "id": "custom.width",
                "value": 80
              }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "Node",
            "desc": false
          }
        ],
        "footer": {
          "show": false
        }
      },
      "transformations": [
        {
          "id": "seriesToColumns",
          "options": {
            "byField": "instance"
          }
        },
        {
          "id": "filterFieldsByName",
          "options": {
            "include": {
              "names": [
                "instance",
                "Value #C",
                "Value #D",
                "Value #F",
                "Value #G",
                "Value #H",
                "Value #I",
                "Value #K",
                "Value #L",
                "Value #M",
                "Value #N",
                "nodename 1"
              ]
            }
          }
        },
        {
          "id": "organize",
          "options": {
            "renameByName": {
              "instance": "Node",
              "Value #C": "CPU %",
              "Value #D": "Mem %",
              "Value #F": "Disk %",
              "Value #G": "Disk Read",
              "Value #H": "Disk Write",
              "Value #I": "TCP Estab",
              "Value #K": "Net RX",
              "Value #L": "Net TX",
              "Value #M": "Load 1m",
              "Value #N": "Sockets",
              "nodename 1": "Hostname"
            },
            "indexByName": {
              "instance": 0,
              "nodename 1": 1,
              "Value #C": 2,
              "Value #D": 3,
              "Value #F": 4,
              "Value #M": 5,
              "Value #G": 6,
              "Value #H": 7,
              "Value #K": 8,
              "Value #L": 9,
              "Value #I": 10,
              "Value #N": 11
            }
          }
        }
      ],
      "targets": [
        {
          "expr": "node_uname_info{job=~\"$job\"}",
          "format": "table",
          "instant": true,
          "refId": "A"
        },
        {
          "expr": "(1 - avg(rate(node_cpu_seconds_total{job=~\"$job\", mode=\"idle\"}[1m])) by (instance)) * 100",
          "format": "table",
          "instant": true,
          "refId": "C"
        },
        {
          "expr": "(1 - (node_memory_MemAvailable_bytes{job=~\"$job\"} / node_memory_MemTotal_bytes{job=~\"$job\"})) * 100",
          "format": "table",
          "instant": true,
          "refId": "D"
        },
        {
          "expr": "max((1 - (node_filesystem_free_bytes{job=~\"$job\", fstype!~\"tmpfs|overlay\"} / node_filesystem_size_bytes{job=~\"$job\", fstype!~\"tmpfs|overlay\"})) * 100) by (instance)",
          "format": "table",
          "instant": true,
          "refId": "F"
        },
        {
          "expr": "max(rate(node_disk_read_bytes_total{job=~\"$job\"}[1m])) by (instance)",
          "format": "table",
          "instant": true,
          "refId": "G"
        },
        {
          "expr": "max(rate(node_disk_written_bytes_total{job=~\"$job\"}[1m])) by (instance)",
          "format": "table",
          "instant": true,
          "refId": "H"
        },
        {
          "expr": "node_netstat_Tcp_CurrEstab{job=~\"$job\"}",
          "format": "table",
          "instant": true,
          "refId": "I"
        },
        {
          "expr": "max(rate(node_network_receive_bytes_total{job=~\"$job\"}[1m])*8) by (instance)",
          "format": "table",
          "instant": true,
          "refId": "K"
        },
        {
          "expr": "max(rate(node_network_transmit_bytes_total{job=~\"$job\"}[1m])*8) by (instance)",
          "format": "table",
          "instant": true,
          "refId": "L"
        },
        {
          "expr": "avg(node_load1{job=~\"$job\"}) by (instance) / count(node_cpu_seconds_total{job=~\"$job\", mode=\"system\"}) by (instance)",
          "format": "table",
          "instant": true,
          "refId": "M"
        },
        {
          "expr": "node_sockstat_sockets_used{job=~\"$job\"}",
          "format": "table",
          "instant": true,
          "refId": "N"
        }
      ]
    }
  ],
  "schemaVersion": 39,
  "version": 1
}
