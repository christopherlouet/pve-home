{
  "id": null,
  "uid": "nodes-overview",
  "title": "Nodes Overview",
  "description": "Single-pane overview of all monitored nodes with key metrics. Click a hostname to drill down to Node Exporter Full.",
  "tags": [
    "node-exporter",
    "overview"
  ],
  "timezone": "browser",
  "editable": false,
  "graphTooltip": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "job",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "query": "label_values(node_uname_info, job)",
        "definition": "label_values(node_uname_info, job)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        }
      }
    ]
  },
  "panels": [
    {
      "title": "Node Resources Overview",
      "type": "table",
      "gridPos": {
        "h": 16,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "cellOptions": {
              "type": "auto"
            },
            "inspect": false
          },
          "links": [
            {
              "title": "Drill down to Node Exporter Full",
              "url": "/d/node-exporter?var-node=${__data.fields.Node}&from=${__from}&to=${__to}",
              "targetBlank": false
            }
          ]
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Node"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 150
              },
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "color-text"
                }
              },
              {
                "id": "color",
